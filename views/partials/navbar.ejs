      <a class="mr-5 text-black hover:text-gray-400" href="/login" >Sign In</a>
      <a class="mr-5 text-black hover:text-gray-400" href="/register">Register</a>
    </nav>
    <% }else{ %> 
      <nav class="md:ml-auto flex flex-wrap items-center justify-center"></nav>
      <a class="mr-5 text-black hover:text-gray-400" href="/logout">Sign Out</a>
    </nav>
    <% } %> 
