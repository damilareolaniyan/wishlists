<% layout('layouts/boilerplate') %>
<div class="row">
    <h1 class="text-center">Add New Wishlists</h1>
    <div class="col-6 offset-3">
        <form action="/wishlists/newform" class="validated-form" method="POST" >
        <div class="mb-3">
            <label class="form-label" for="wishlist">Name your wishlist</label>
            <input type="text" class="form-control" id="wishlist" name="wishlist[name]"
            placeholder="Name your wishlist"> 
        </div>
            
            <button class="btn btn-primary btn-block mb-3">SAVE</button>
        </form> 
    </div>
</div> 
    


